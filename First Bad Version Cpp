// The API isBadVersion is defined for you.
// bool isBadVersion(int version);

class Solution {
public:
    int firstBadVersion(int n) {
        if(n==1) return 1;
        int i=0, j =n, mid=0;
        while(i<j){
            mid = i + (j-i) / 2;
            if(isBadVersion(mid)) j = mid;
            else i = mid+1;
        }
        //if(!isBadVersion(mid)) return mid+1;
        return i;
    }
};
